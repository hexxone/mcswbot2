<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <StartupObject>mcswbot2.Program</StartupObject>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile>hexxon_me_CA.pfx</AssemblyOriginatorKeyFile>
    <Version>2.1.0</Version>
    <Description>MineCraftServerWatchBotV2</Description>
    <Copyright></Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>2.1.0.0</AssemblyVersion>
    <RepositoryUrl>http://github.com/Hexxonite/mcswbot2</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BooruSharp" Version="1.4.8" />
    <PackageReference Include="mcswlib" Version="2.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="ScottPlot" Version="4.0.22" />
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
    <PackageReference Include="Telegram.Bot" Version="15.4.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Imazen.WebP">
      <HintPath>..\..\libwebp-net\src\Imazen.WebP\bin\Debug\netstandard2.0\Imazen.WebP.dll</HintPath>
    </Reference>
    <Reference Include="ZufallSatz">
      <HintPath>..\..\ZufallSatz\ZufallSatz\bin\Debug\netstandard2.0\ZufallSatz.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="IncludeBuild\fonts\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

</Project>
